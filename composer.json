{
  "name": "xeroticikot/xeroticframe",
  "description": "A lightweight PHP 7.0 framework",
  "authors": [
      {
          "name": "Mahmud Abdur Rahman",
          "email": "clanlord.ikot@gmail.com"
      }
  ],
  "minimum-stability": "dev",
  "keywords": ["framework", "xerotic"],
  "license": "GPL-3.0",
  "type": "project",
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "xerotic\\": "bin/",
      "xlibs\\": "libs/",
      "xcontrollers\\": "controllers/",
      "xmodels\\": "models/"
    },
    "classmap": [
      "bin/",
      "libs/",
      "controllers/",
      "models/"
    ]
  },
    "require": {
        "php": ">=7.0.0",
        "jenssegers/blade": "^1.1",
        "illuminate/database": "^5.5",
        "robmorgan/phinx": "^0.9.1",
        "illuminate/validation" : "^5.5"
    }
}
